// Place your key bindings in this file to overwrite the defaults
[
    { "key": "cmd+b", "command": "gitlens.toggleFileBlame",
                       "when": "editorTextFocus && gitlens:isTracked" },
    { "key": "cmd+alt+b", "command": "gitlens.toggleLineBlame" },
    { "key": "cmd+\\",  "command": "workbench.action.toggleSidebarVisibility" },
    //{ "key": "cmd+shift+B",  "command": "HookyQR.beautifyFile" },
    { "key": "cmd+shift+B",  "command": "gitlens.toggleLineBlame" },

    { "key": "cmd+f2",       "command": "bookmarks.toggle",            "when": "editorTextFocus" },
    { "key": "shift+f2",     "command": "bookmarks.jumpToPrevious",    "when": "editorTextFocus" },
    { "key": "f2",           "command": "bookmarks.jumpToNext",        "when": "editorTextFocus" },
    { "key": "cmd+shift+f2", "command": "bookmarks.list",              "when": "editorTextFocus" },
    { "key": "ctrl+m",       "command": "editor.action.jumpToBracket", "when": "editorTextFocus"},
    { "key": "shift+cmd+\\", "command": "workbench.files.action.showActiveFileInExplorer"}
]